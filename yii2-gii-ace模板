<?php
echo '<?php
$this->registerJs(\' 
    function del(ids){        
    var to_data = {ids:ids,"_csrf-backend":"\'.(yii::$app->request->csrfToken).\'"};        
    $.ajax({
            type: "POST",
            url:"\'.Url::toRoute([\'delete\']).\'",            
            data: to_data,
            dataType:"json",
            success: function(data) {
                bootbox.dialog({
                    message: data.message,
                });
                if(data.status=="1"){
                    setTimeout(function() {window.location.reload()}, 1000);
                }
            }
        });
    };
    $(".do-del").on("click",function(){
        var id = $(this).attr("data-id");
        bootbox.confirm("请确认删除?", function(result) {
            if(result){
                del([id]);
            }
        });
        
    });
    $(".do-dels").on(ace.click_event,function() {
            bootbox.confirm("请确认删除?", function(result) {
                if(result) {
                    var ids = [];
                    var check_ids = $(".del-ids:checked");
                    if(check_ids.size()>0){
                        for(i = 0; i < check_ids.size(); i++){
                            var id = check_ids.eq(i).val();
                                if(id != ""){
                                ids.push(id);
                            }
                        }
                    }
                    if(ids.length==0){
                        alert("请选择记录！");
                    }
                    del(ids);
                    
                }
            });
        });
        
    \');
?>
';
