                <ul class="list-unstyled spaced">
                    <?='<?php'?> foreach ($errorArr as $err_key=>$err_info) { ?>
                        <li>
                            <i class="ace-icon fa fa-times bigger-110 red"></i>
                            <?='<?='?>implode(',', $err_info)  ?>
                        </li>
                    <?='<?php'?> } ?>
                </ul>
            </div>
            <?='<?php'?> } ?>
            
            <?='<?php'?> $form = ActiveForm::begin([
                'options' => [
                    'class'=>'form-horizontal'
                ],
            ]); ?>

			<?php foreach ($generator->getTableSchema()->columns as $col_key=>$col_vo) { ?>
			<div class="form-group">
			    <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> <?=$col_vo->comment?$col_vo->comment:$col_key?> </label>

			    <div class="col-sm-9">
			        <input name="<?='<?='?>$modelName?>[<?=$col_key?>]" value="<?='<?='?>$model-><?=$col_key?>?>" type="text" id="form-field-1"  class="col-xs-10 col-sm-5" />
			    </div>
			</div>      
			<?php } ?>
            <div class="clearfix form-actions">
                <div class="col-md-offset-3 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="ace-icon fa fa-check bigger-110"></i>
                        Submit
                    </button>
                </div>
            </div>

        <?='<?php'?> ActiveForm::end(); ?>

    </div>
</div>






